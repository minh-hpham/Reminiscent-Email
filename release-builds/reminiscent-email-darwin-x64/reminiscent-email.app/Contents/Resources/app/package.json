{
  "name": "reminiscent-email",
  "description": "* This repos is for Reminiscent-Email project  - a cross platform desktop application using Electron and Python (for Machine Learning components).  ``` * Clone the repository ``` $ git clone https://github.com/minh-hpham/Reminiscent-Email.git ``` ``` * Install project requirements and dependencies ``` $ npm install -r requirements.txt",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . reminiscent-email-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Reminiscent Email App\"",
    "package-linux": "electron-packager . reminiscent-email-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "repository": "https://github.com/minh-hpham/Reminiscent-Email.git",
  "keywords": [
    "Reminiscent",
    "Electron",
    "Minh Pham"
  ],
  "author": "Minh Pham",
  "dependencies": {
    "express": "^4.16.2",
    "google-auth-library": "^0.12.0",
    "googleapis": "^25.0.0",
    "is-online": "^7.0.0",
    "jQuery": "^1.7.4",
    "jquery": "^3.3.1"
  },
  "devDependencies": {
    "electron": "^1.8.4",
    "electron-packager": "^11.2.0"
  }
}
